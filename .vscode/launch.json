    "version": "0.2.0",
    "configurations": [
        {
            "name": "go-hello-world_cli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/cmd/cli/main.go",
            "env": {
                //
            }
        },
        {
            "name": "go-hello-world_rest",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/cmd/rest/main.go",
            "env": {
                "CERT_FILE": "./certs/ssl.crt",
                "KEY_FILE": "./certs/ssl.key",
                "HTTPS_ENABLED": "true",
                "HTTP_PORT": "8080",
            }
        },
    ]
}