    "version": "0.2.0",
    "configurations": [
        {
            "name": "CLI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceRoot}/cmd/cli",
            "program": "${workspaceRoot}/cmd/cli/main.go",
            "env": {
                //
            }
        },
        {
            "name": "REST (Remote)",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1"
        },
        {
            "name": "REST",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceRoot}/cmd/rest",
            "program": "${workspaceRoot}/cmd/rest/main.go",
            "env": {
                //
            }
        },
    ]
}